    "commit": "git-cz"
  },
  "dependencies": {
    "@astrojs/image": "^0.17.2",
    "@astrojs/sitemap": "^2.0.1",
    "@astrojs/vercel": "^3.7.4",
    "astro": "^2.9.6",
    "sharp": "^0.32.4"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.40.0",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@iconify/json": "^2.2.95",
    "@typescript-eslint/parser": "^6.2.0",
    "@unocss/eslint-config": "^0.54.0",
    "@unocss/reset": "^0.54.0",
    "@unocss/transformer-variant-group": "^0.54.1",
    "commitizen": "^4.3.0",
    "eslint": "^8.46.0",
    "eslint-plugin-astro": "^0.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "husky": "^8.0.0",
    "inquirer": "9",
    "lint-staged": "^13.2.3",
    "sass": "^1.64.2",
    "unocss": "^0.54.0"
  },
  "lint-staged": {
    "*.{astro,ts,json,cjs,md}": "npx eslint . --fix --ignore-path .gitignore"
